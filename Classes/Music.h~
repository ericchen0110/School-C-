#include <iostream>

#ifndef PARENT_H
#define PARENT_H
#include "Parent.h"
#endif

class music : public parent
{
 public:
  music(char* title, int year, char* artist, char* publisher, char* duration);
  virtual char* get_artist();
  virtual char* get_publisher();
  virtual char* get_duration();
 private:
  char* artist;
  char* publisher;
  char* duration;
};
